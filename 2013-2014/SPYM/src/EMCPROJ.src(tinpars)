         XR    R2,R2                                                    13
         LHI   R2,ICB#           CREATE ICB  RECORD                     00126513
       GETMAIN R,LV=(R2)                                                00126613
         ST    R1,RICB@

         XR    R2,R2
         LHI   R2,PARAMS#        CREATE ICB  RECORD
        GETMAIN R,LV=(R2)
         ST    R1,RPARAMS@
                                                                        00126813
    CREATERECB (R6),RECB@
         ST    R6,CRECB@

         OPEN  (DEBUG,OUTPUT)
         LR    R0,R11
      PROCCALL NAME=INPARSE,(R10)

         LA    R9,MEMAREA
